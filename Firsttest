<script type="text/javascript">


var smugmugDomain = "https://kaydinadmin.smugmug.com/Client-Login";


function getQueryVariable(variable)
{
       var query = window.location.search.substring(1);
       var vars = query.split("&");
       for (var i=0;i<vars.length;i++) {
               var pair = vars[i].split("=");
               if(pair[0] == variable){return pair[1];}
       }
       return(false);
}

function opensmugLinkWindow(smugid)
  	{
		
		
		
		var smugmugbase = "https://kaydinadmin.smugmug.com/ClientGalleries/";
		var smugidname = getQueryVariable("customerid");
		
		
		if (smugidname  == "")
	{
		var smugidname = "notfound";
		
		

	}
		var URL = smugmugbase + smugidname;
		window.location=URL;
	
	}
	
</script>

<script type="text/javascript">

getQueryVariable("mode");
getQueryVariable("customerid");
opensmugLinkWindow();


    
 
</script>
